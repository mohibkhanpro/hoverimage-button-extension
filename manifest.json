{
  "manifest_version": 3,
  "name": "Image Hover Extension",
  "version": "1.0",
  "description": "Show button when hovering over images",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {}
}
